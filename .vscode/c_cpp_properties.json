{
  "configurations": [
    {
      "name": "Mac",
      "macFrameworkPath": [
        "/System/Library/Frameworks",
        "/Library/Frameworks"
      ],
      "intelliSenseMode": "macos-clang-x64",
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c17",
      "cppStandard": "c++14",
      "includePath": [ "${workspaceFolder}/build" ],
      "defines": [ "HAVE_CONFIG_H", "_COMPILING_BOSL" ],
      "forcedInclude": [ "${workspaceFolder}/config.h" ]
    },
    {
      "name": "Linux",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++14",
      "includePath": [ "${workspaceFolder}/build" ],
      "defines": [ "HAVE_CONFIG_H", "_COMPILING_BOSL" ],
      "forcedInclude": [ "${workspaceFolder}/config.h" ]
    }
  ],
  "version": 4
}
